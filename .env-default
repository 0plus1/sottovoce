RTSTT_MODEL=base.en           # default: base.en
RTSTT_COMPUTE_TYPE=default    # default: default; e.g., int8/int8_float16 to avoid float16 warnings on CPU
RTSTT_LANGUAGE=               # default: empty = auto
RTSTT_USE_MICROPHONE=true     # true/false, default: true
RTSTT_LOG_LEVEL=NONE          # INFO, WARNING, ... (NONE disables logging)